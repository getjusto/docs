          "comment": {
            "type": "string",
            "example": "Sin azúcar",
            "description": "Comentario del item."
          },
          "type": {
            "type": "string",
            "enum": [
              "product",
              "deliveryFee"
            ],
            "description": "Tipo de item."
          },
          "sentToKDSAt": {
            "type": "string",
            "example": "2020-01-01T00:00:00.000Z",
            "format": "date-time",
            "description": "Fecha en la que se envió a cocina."
